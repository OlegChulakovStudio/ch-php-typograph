{
  "name": "chulakov/ch-php-typograph",
  "description": "Component lets tune Muravjev typograph",
  "version": "1.0.0",
  "type": "php-typograph-component",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=5.6",
    "oleg-chulakov-studio/mdash": "1.0.0"
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Chulakov Studio",
      "email": "hello@chulakov.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "chulakov\\phptypograph\\": "src/"
    }
  },
